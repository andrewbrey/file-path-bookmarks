{
  "name": "file-path-bookmarks",
  "description": "A simple file path bookmark management utility",
  "version": "1.1.0",
  "author": "Andrew Brey <34140052+andrewbrey@users.noreply.github.com>",
  "license": "MIT",
  "homepage": "https://github.com/andrewbrey/file-path-bookmarks",
  "repository": "andrewbrey/file-path-bookmarks",
  "bugs": "https://github.com/andrewbrey/file-path-bookmarks/issues",
  "main": "lib/index.js",
  "scripts": {
    "dev": "./bin/run",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "1.2.4",
    "chalk": "4.1.1",
    "cli-ux": "5.6.3",
    "fs-extra": "10.0.0",
    "fuzzy": "0.1.3",
    "inquirer": "8.1.2",
    "lodash": "4.17.21",
    "ts-dedent": "2.1.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/fs-extra": "9.0.12",
    "@types/inquirer": "7.3.3",
    "@types/lodash": "4.14.171",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "globby": "^11",
    "prettier": "2.3.2",
    "ts-node": "^10",
    "typescript": "^4.3"
  },
  "bin": {
    "file-path-bookmarks": "./bin/run"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "file-path-bookmarks",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "dirname": "file-path-bookmarks"
  },
  "types": "lib/index.d.ts",
  "keywords": [
    "oclif"
  ]
}
